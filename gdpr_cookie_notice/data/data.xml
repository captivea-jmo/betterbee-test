            <field name="name">Remember social media settings</field>
        </record>
        <record id="group_highlight_7" model="cookie.popup.group.info">
            <field name="name">Remember selected region and country</field>
        </record>
        <record id="group_highlight_8" model="cookie.popup.group.info">
