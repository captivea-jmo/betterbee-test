<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Data of cookie.popup.group.info  -->
        <record id="group_highlight_1" model="cookie.popup.group.info">
            <field name="name">Remember your cookie permission setting</field>
        </record>
        <record id="group_highlight_2" model="cookie.popup.group.info">
