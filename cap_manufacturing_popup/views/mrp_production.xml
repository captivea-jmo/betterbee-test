<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="manufacturing_order_custom_form" model="ir.ui.view">
            <field name="name">mrp.production.custom.form</field>
            <field name="model">mrp.production</field>
            <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
            <field name="arch" type="xml">
                <xpath expr='//notebook' position="inside">
                    <page string="Products Manufactured" name="products_manufactured_page">
                        <group name="products_manufactured_group_1" >
                            <field name="product_manufactured_ids" string="Outcomes" />
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>