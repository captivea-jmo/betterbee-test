                    <a t-attf-class="nav-link #{'active' if cat_lvl == 1 else ''}" t-attf-id="d-#{r}-tab" data-toggle="tab" t-attf-href="#d-#{r}" role="tab" aria-controls="home" aria-selected="true">
                      <span>Menu 1</span>
                    </a>
                  </li>
                </t>
              </ul>
            </div>
            <div class="col-md-9">
              <div class="row main_cat_content">
                <div class="col">
                  <div class="tab-content" id="gt_TabContent">
                    <t t-set="cat_lvl" t-value="0"/>
                    <t t-foreach="range(5)" t-as="r">
                      <t t-set="cat_lvl" t-value="cat_lvl+1"/>
                      <div t-attf-class="tab-pane fade #{'show active' if cat_lvl == 1 else ''}" t-attf-id="d-#{r}" role="tabpanel" t-attf-aria-labelledby="d-#{r}-tab">
                        <t t-foreach="range(30)" t-as="i">
                          <div class="levelclass">
                            <a href="/">Menu <t t-esc="i"/></a>
                          </div>
                        </t>
                      </div>
                    </t>
                  </div>
                </div>
              </div>
              <div class="row offer_row">
                <div class="col">
                  <div class="offer_footer">
                    <div class="row">
                      <div class="col">
                        <img src="/web/image/website.s_mega_menu_menu_image_menu_default_image"/>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </t>
      </section>
    </template>

