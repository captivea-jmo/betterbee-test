<odoo>
  <data>

    <template id="categories_temp" name="Category Holder">
      <div t-attf-class="col-md#{col_size} cat_main cat_main_temp">
				<strong>
          <span class="mega_cat_image mega_cat_image_temp">
            <img src="/website_mega_menus/static/src/img/mm5.jpg" class="img img-fluid"/>
          </span>
					<a class="nav-link" href="#">
            Menu 1
					</a>
				</strong>
				<div class="child_categories child_cat_temp">
					<t t-foreach="range(4)" t-as="c1">
						<t t-set="level" t-value="0"/>
            <div class="levelclass lvl_temp" t-att-level="level">
      				<a href="#">
      					Menu
      				</a>
              <!-- <span class="fa fa-caret-right"/> -->
      			</div>
					</t>
				</div>
			</div>
    </template>

    <template id="categories_recursive_child" name="Category Child list">
      <!-- <style>
        .levelclass:hover > a{color: body_hover_color !important;}
      </style> -->
			<div class="levelclass" t-att-level="level">
				<span class="bullet-point fa fa-angle-double-right"></span>
				<a t-attf-href="/shop/category/#{slug(c1)}" t-attf-style="color:##{body_color or '212121'}" t-attf-target="#{'_blank' if new_window else ''}">
					<t t-esc="c1.name"/>
				</a>
        <t t-if="c1.child_id">
          <span class="fa fa-caret-right"/>
          <t t-set="level" t-value="level+1"/>
          <div class="grand_child">
            <t t-foreach="c1.child_id" t-as="c2">
              <t t-set="c1" t-value="c2"/>
              <t t-call="website_mega_menus.categories_recursive_child"/>
            </t>
          </div>
        </t>
			</div>
		</template>

		<template id="categories_recursive" name="Category list">
			<div t-attf-class="col-md#{col_size} cat_main">
				<strong>
					<span t-if="c.image_512" t-field="c.image_512" class="mega_cat_image" t-options="{'widget': 'image'}"></span>
					<a class="nav-link" t-attf-href="/shop/category/#{slug(c)}" t-attf-style="color:##{root_color or '212121'}" t-attf-target="#{'_blank' if new_window else ''}"><t t-esc="c.name"/>
            <span class="fa fa-caret-right"/>
					</a>
				</strong>
				<div t-if="c.child_id" class="child_categories">
					<t t-foreach="c.child_id" t-as="c1">
						<t t-set="level" t-value="0"/>
						<t t-call="website_mega_menus.categories_recursive_child"/>
					</t>
				</div>
			</div>
		</template>

    <template id="wk_product_card" name="Product Card">
      <t t-if="product">
        <t t-set="product_href" t-value="product.website_url" />
        <t t-set="combination_info" t-value="product._get_combination_info(only_template=True, add_qty=add_qty or 1, pricelist=pricelist)"/>
        <div class="card">
          <a t-att-href="product_href" class="d-block" itemprop="url">
            <span t-field="product.image_1920"
              t-options="{'widget': 'image', 'preview_image': 'image_1024' if product_image_big else 'image_256'}"
              class="d-flex h-100 justify-content-center align-items-center"/>
          </a>
          <div class="card-body">
            <div class="o_wsale_product_information_text">
              <h6 class="o_wsale_products_item_title">
                <a itemprop="name" t-att-href="product_href" t-att-content="product.name" t-field="product.name" />
                <a role="button" t-if="not product.website_published" t-att-href="product_href" class="btn btn-sm btn-danger" title="This product is unpublished.">Unpublished</a>
              </h6>
              <div class="product_price" itemprop="offers" itemscope="itemscope" itemtype="http://schema.org/Offer">
                <!-- <t t-esc="website.read()"/> -->
                <!-- <t t-esc="website.currency_id"/> -->
                <del t-attf-class="text-danger mr-2 {{'' if combination_info['has_discounted_price'] else 'd-none'}}" style="white-space: nowrap;" t-esc="combination_info['list_price']" t-options="{'widget': 'monetary', 'display_currency': website.company_id.currency_id}" />
                <span t-if="combination_info['price']" t-esc="combination_info['price']" t-options="{'widget': 'monetary', 'display_currency': website.company_id.currency_id}"/>
                <!-- <span>$</span><span t-if="combination_info['price']" t-esc="combination_info['price']"/> -->
                <span itemprop="price" style="display:none;" t-esc="combination_info['price']" />
                <span itemprop="priceCurrency" style="display:none;" t-esc="website.company_id.currency_id.name" />
              </div>
            </div>
          </div>
        </div>
      </t>

      <t t-else="">
        <div class="card">
          <a href="/" class="d-block" itemprop="url">
            <span class="d-flex h-100 justify-content-center align-items-center">
              <img src="/website_mega_menus/static/src/img/img-1-mega-menu-1.png" class="img img-fluid"/>
            </span>
          </a>
          <div class="card-body">
            <div class="o_wsale_product_information_text">
              <h6 class="o_wsale_products_item_title">
                <a href="#">Product Name</a>
              </h6>
              <div class="product_price">
                <span>$450.00</span>
              </div>
            </div>
          </div>
        </div>
      </t>
    </template>

    <template id="wk_carousel_main" name="Wk Carousel">
      <div class="row">
        <div class="col-8" style="align-self: flex-end;">
          <h3 style="text-align: left !important"><t t-esc="carousel_heading"/></h3>
