<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
        <record id="res_config_settings_view_form_inherited" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='social_media_settings']" position="after">
                    <div class="col-12 col-lg-6 o_setting_box" id="add_to_cart_ajax">
                        <div class="o_setting_left_pane">
                            <field name="is_cart_ajax_field"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="is_cart_ajax_field"/>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>